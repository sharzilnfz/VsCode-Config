// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+shift+alt+cmd+o",
    "command": "extension.liveServer.goOnline",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+s",
    "command": "editor.action.sourceAction",
    "args": {
      "kind": "source.addMissingImports",
      "apply": "first"
    }
  },

  {
    "key": "cmd+l cmd+o",
    "command": "-extension.liveServer.goOnline",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+home",
    "command": "extension.liveServer.goOffline",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+l cmd+c",
    "command": "-extension.liveServer.goOffline",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+backspace",
    "command": "editor.action.sourceAction"
  },
  {
    "key": "ctrl+shift+alt+cmd+pagedown",
    "command": "editor.emmet.action.balanceIn"
  },
  {
    "key": "ctrl+shift+alt+cmd+pageup",
    "command": "editor.emmet.action.balanceOut"
  },
  {
    "key": "ctrl+shift+alt+cmd+enter",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "ctrl+shift+alt+cmd+f10",
    "command": "monokai_pro.select_theme"
  }
]
